{
  "name": "design-system-techatpark",
  "version": "v6.0.0-beta.21",
  "private": true,
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "main": "index.js",
  "scripts": {
    "serve": "browser-sync start --config browser-sync.js",
    "build": "run-s build-storybook build-assets",
    "copy": "copy-files-from-to --when-file-exists overwrite",
    "sass": "sass src/scss:dist/css",
    "build-assets": "run-s copy sass",
    "build-storybook": "build-storybook -o dist/storybook",
    "generate-addon-jest-testresults": "jest --config=tests/addon-jest.config.json --json --outputFile=stories/addon-jest.testresults.json",
    "watch:scss": "onchange \"src/scss\" -- run-s sass",
    "watch:html": "onchange \"src/index.html\" -- run-s copy",
    "watch:images": "onchange \"src/images\" -- run-s copy",
    "storybook": "start-storybook -s ./dist/storybook -p 9006",
    "start": "run-p build storybook watch:* serve",
    "postinstall": "run-s build"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "v6.0.0-beta.21",
    "@storybook/addon-actions": "v6.0.0-beta.21",
    "@storybook/addon-backgrounds": "v6.0.0-beta.21",
    "@storybook/addon-docs": "v6.0.0-beta.21",
    "@storybook/addon-events": "v6.0.0-beta.21",
    "@storybook/addon-jest": "v6.0.0-beta.21",
    "@storybook/addon-knobs": "v6.0.0-beta.21",
    "@storybook/addon-links": "v6.0.0-beta.21",
    "@storybook/addon-storyshots": "v6.0.0-beta.21",
    "@storybook/addon-storysource": "v6.0.0-beta.21",
    "@storybook/addon-viewport": "v6.0.0-beta.21",
    "@storybook/addons": "v6.0.0-beta.21",
    "@storybook/client-api": "v6.0.0-beta.21",
    "@storybook/core": "v6.0.0-beta.21",
    "@storybook/core-events": "v6.0.0-beta.21",
    "@storybook/html": "v6.0.0-beta.21",
    "@storybook/source-loader": "v6.0.0-beta.21",
    "bootstrap": "^4.5.0",
    "browser-sync": "^2.26.7",
    "copy-files-from-to": "^2.1.0",
    "eventemitter3": "^4.0.0",
    "format-json": "^1.0.3",
    "global": "^4.3.2",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "postcss-color-rebeccapurple": "^5.0.0",
    "sass": "^1.26.7"
  }
}
